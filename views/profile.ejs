<html>
  <head>
    <title>Profile page</title>
  </head> 
  <body>
    <!-- including html page -->
    <%- include('./header') %>
    <!-- <%= %> to use a variable -->
    <h1>Welcome <%= data.name %></h1>
    <h3><%= data.email %></h3>
    <ul>
      <!-- showing multiple data -->
      <% data.skills.forEach((item)=>{ %>
        <li> <%= item %> </li>
      <% }) %> 
    </ul>  
  </body>
</html>